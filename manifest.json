{
  "manifest_version": 3,
  "name": "Auto-Pin Domains",
  "version": "1.0",
  "description": "Auto-pin tabs based on domain list.",
  "permissions": ["storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "options.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
